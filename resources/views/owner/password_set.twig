    <body class=" login">
        <!-- BEGIN : LOGIN PAGE 5-1 -->
        <div class="user-login-5">
            <div class="row bs-reset">
                <div class="col-md-6 bs-reset mt-login-5-bsfix">
                    <div class="login-bg" style="background-image:url(/assets/pages/img/login/bg1.jpg)">
                        <img style="max-width: 200px;" class="login-logo" src="/assets/images/TommysPM_Logo.png" /> </div>
                </div>
                <div class="col-md-6 login-container bs-reset mt-login-5-bsfix">
                    <div class="login-content">
                        <h1>Set Password</h1>
                        <strong>Your password must contain at least 1 uppercase letter, 1 lowercase letter and minimum 8 characters.</strong> 
                            <form action="{{ path_for('passwordSetFromLink', {'ownerid' : ownerid, 'ownercode' : ownercode,'token' : token}) }}" class="login-form" method="post" data-parsley-excluded="input[type=button], input[type=submit], input[type=reset], input[type=hidden], [disabled], :hidden" data-parsley-trigger="keyup" data-parsley-validate>
                            <div class="row">
                                <div class="form-group{{ errors.password ? ' has-error' : '' }}">
                                    <label for="password">Password : </label>
                                    <input id="password" class="form-control form-control-solid placeholder-no-fix form-group" type="password" autocomplete="off" name="password" required="required" data-parsley-errors-container=".errorspannewpassinput" data-parsley-minlength="8"
    data-parsley-required-message="Please enter new password."
    data-parsley-uppercase="1"
    data-parsley-lowercase="1"
    data-parsley-required/>
                                    {% if errors.password %}
                                    <span class="help-block">{{ errors.password | first }}</span>
                                    {% endif %}
                                     <span class="help-block errorspannewpassinput"></span> 
                                </div>
                                <div class="form-group{{ errors.confirm_password ? ' has-error' : '' }}">
                                    <label for="confirm_password">Confirm Password : </label>
                                    <input id="confirm_password" class="form-control form-control-solid placeholder-no-fix form-group" type="password" autocomplete="off" name="confirm_password" required="required" data-parsley-errors-container=".errorspanconfirmnewpassinput" data-parsley-minlength="8"
    data-parsley-required-message="Please re-enter new password."
    data-parsley-equalto="#password"
    data-parsley-required/>
                                    {% if errors.confirm_password %}
                                    <span class="help-block">{{ errors.confirm_password | first }}</span>
                                    {% endif %}
                                     <span class="help-block errorspanconfirmnewpassinput"></span> 
                                </div>
                            </div>
                            <div class="row">
                                
                                <div class="form-group text-left">
                                    <button style="color:#333;" class="btn btn-tommy action-btn" type="submit">Set Password</button>
                                </div>
                            </div>
                             {{ csrf.field | raw }}
                        </form>
                    </div>
                    <div class="login-footer">
                        <div class="row bs-reset">
                            <div class="col-xs-5 bs-reset">
                                <ul class="login-social">
                                    <li>
                                        <a href="javascript:;">
                                            <i class="icon-social-facebook"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="javascript:;">
                                            <i class="icon-social-twitter"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="javascript:;">
                                            <i class="icon-social-dribbble"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="col-xs-7 bs-reset">
                                <div class="login-copyright text-right">
                                    <p>Copyright &copy; Tommy's Real Estate</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END : LOGIN PAGE 5-1 -->
